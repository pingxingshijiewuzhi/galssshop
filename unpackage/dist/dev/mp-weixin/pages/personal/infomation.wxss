
page {
	background-color: #F4F4F4;
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.top {
	width: 100%;
	height: 120rpx;
	border-bottom: 1px solid #E5E5E5;
	background-color: #FFFFFF;
	margin-top: 20rpx;
}
.top_right {
	width: 20%;
	margin-right: 30rpx
}
.top_left {
	letter-spacing: 4rpx;
	margin-left: 30rpx;
}
.top_avatar {
	width: 100rpx;
	height: 100rpx;
	border-radius: 50% 50%;
}
.name {
	width: 150rpx;
	text-align: center;
}
.names {
	width: 400rpx;
	text-align: right;
}
.top_next {
	width: 16rpx;
	height: 26rpx
}
.list {
	width: 100%;
	height: 120rpx;
	border-bottom: 1px solid #E5E5E5;
	background-color: #FFFFFF;
	margin-top: 20rpx;
}
.list_r {
	width: 160rpx;
	margin-left: 30rpx;
}
.list_new {
	width: 21%;
	margin-right: 30rpx
}
.list_img {
	width: 50rpx;
	height: 50rpx
}

