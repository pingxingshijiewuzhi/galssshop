
page {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.top {
	width: 97%;
	height: 360rpx;
	margin: 30rpx auto;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.money {
	width: 100%;
	height: 160rpx;
	color: #FFFFFF;
	font-size: 40rpx;
	letter-spacing: 4rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: end;
	-webkit-align-items: flex-end;
	        align-items: flex-end;
}
.number {
	font-size: 50rpx;
}
.top_list {
	width: 53%;
	height: 160rpx;
	margin: 0 auto
}
.top_l {
	width: 160rpx;
	height: 56rpx;
	line-height: 56rpx;
	text-align: center;
	background-color: #F2F2F2;
	border: 1px solid #F2F2F2;
	border-radius: 28rpx;
}
.top_r {
	width: 160rpx;
	height: 56rpx;
	line-height: 56rpx;
	text-align: center;
	border-radius: 28rpx;
	border: 1px solid #FFFFFF
}
.middle {
	width: 100%;
	height: 500rpx;
	margin-top: 60rpx;
}
.middle_l {
	width: 94%;
	height: 132rpx;
	margin: 30rpx auto;
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.left {
	width: 34%;
	height: 120rpx;
	margin-left: 30rpx
}
.left image {
	width: 58rpx;
	height: 52rpx;
	margin-right: 36rpx
}
.next_img {
	width: 16rpx;
	height: 26rpx;
	margin-right: 40rpx
}

