<view class="content"><view class="list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="lis_one flexs1"><view class="left"><block wx:if="{{item.type==1}}"><view class="title">{{"微信"+item.remark}}</view></block><block wx:if="{{item.type==2}}"><view class="title">{{"支付宝"+item.remark}}</view></block><block wx:if="{{item.type==3}}"><view class="title">{{"银行卡"+item.remark}}</view></block><block wx:if="{{item.type==4}}"><view class="title">{{"其他"+item.remark}}</view></block><view class="times">{{item.createtime}}</view></view><view class="right text-right"><view class="text-colors">{{"￥"+item.money}}</view><block wx:if="{{item.status==1}}"><view class="text-orange">处理中</view></block><block wx:if="{{item.status==2}}"><view class="text-orange">成功</view></block><block wx:if="{{item.status==3}}"><view class="text-orange">失败</view></block></view></view></block></view></view>