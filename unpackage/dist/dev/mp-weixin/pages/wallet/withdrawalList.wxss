}
.lis_one {
	width: 94%;
	height: 126rpx;
	margin: 20rpx auto;
	border-bottom: 1px solid #E5E5E5
}
.left {
	width: 65%;
	height: 100%;
	line-height: 48rpx;
	letter-spacing: 4rpx;
}
.title {
	font-size: 32rpx;
}
.right {
	width: 35%;
	height: 100%;
